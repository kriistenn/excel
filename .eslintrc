{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi":"off",
        "linebreak-style":"off",
        "indent": "off",
        "comma-dangle": "off",
        "no-trailing-spaces": "off",
        "arrow-parens": "off",
        "require-jsdoc": "off",
        "operator-linebreak": "off",
        "max-len":"off"
    },
    "extends": ["eslint:recommended", "google"]
}